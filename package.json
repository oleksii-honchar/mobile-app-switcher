{
  "name": "mobile-app-switcher",
  "version": "0.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "prebuild": "npm run clean:dist",
    "prebuild:loc": "npm run clean:dist",
    "build": "bash scripts/build.sh",
    "build:analyze": "cross-env-shell BUILD_ANALYZE=true bash ./scripts/build.sh",
    "build:loc": "bash scripts/build.loc.sh",
    "build:loc:analyze": "cross-env-shell BUILD_ANALYZE=true bash ./scripts/build.loc.sh",
    "cm": "npx git-cz",
    "check:all": "npm run lint:fix",
    "clean:dist": "rm -rf dist",
    "launch:loc": "npm run launch:dev-server",
    "launch:dev-server": "bash ./scripts/launch.dev-server.sh",
    "lint": "npx eslint src --config eslint.config.js  --ext .js,.jsx,.ts,.tsx,.json --ignore-pattern '/node_modules/*' --ignore-pattern '**/*.test.js' --ignore-pattern '/src/assets/**'",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run launch:loc",
    "test": "cross-env-shell TASK_SITE_NAME=thestar TASK_ENVIRONMENT=localhost node scripts/run-tests.js --env=jsdom --colors",
    "test:coverage": "cross-env TASK_SITE_NAME=thestar TASK_ENVIRONMENT=localhost node scripts/run-tests.js --env=jsdom --colors --coverage",
    "test:cleanup": "trash tests/e2e/reports tests_output",
    "watch": "npm run watch:loc",
    "watch:loc": "bash scripts/watch.loc.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "author": "Oleksii Honchar",
  "license": "MIT",
  "dependencies": {
    "env-cmd": "10.1.0",
    "moment": "2.24.0",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2"
  },
